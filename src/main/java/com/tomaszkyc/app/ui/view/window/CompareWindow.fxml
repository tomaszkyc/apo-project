<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="860.0" prefWidth="1300.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.tomaszkyc.app.ui.view.window.CompareWindowController">
   <children>
      <TabPane fx:id="selectedForCompareImageTabPane" layoutX="-2.0" layoutY="29.0" prefHeight="826.0" prefWidth="650.0" tabClosingPolicy="UNAVAILABLE" />
      <TabPane fx:id="compareWithSelectedImageTabPane" layoutX="648.0" layoutY="29.0" prefHeight="826.0" prefWidth="650.0" tabClosingPolicy="UNAVAILABLE" />
      <MenuBar prefHeight="29.0" prefWidth="1295.0">
        <menus>
          <Menu fx:id="fileMenu" mnemonicParsing="false" text="File">
            <items>
              <MenuItem fx:id="closeWindowMenuItems" mnemonicParsing="false" onAction="#handleClose" text="Close" />
            </items>
          </Menu>
            <Menu fx:id="actionsMenu" mnemonicParsing="false" text="Actions">
              <items>
                <MenuItem fx:id="comparePixelValuesMenuItem" mnemonicParsing="false" onAction="#handleComparingTwoImages" text="Compare images by pixel values" />
              </items>
            </Menu>
        </menus>
      </MenuBar>
   </children>
</AnchorPane>
